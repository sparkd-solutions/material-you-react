{
  "bracketSpacing": true,
  "singleQuote": true,
  "trailingComma": "all",
  "tabWidth": 2,
  "semi": true,
  "printWidth": 120,
  "jsxSingleQuote": true,
  "arrowParens": "always",
  "bracketSameLine": false,
  "jsxBracketSameLine": false,
  "quoteProps": "as-needed",
  "useTabs": false,
  "endOfLine": "crlf",
  "proseWrap": "preserve",
  "htmlWhitespaceSensitivity": "css",
  "singleAttributePerLine": true
}
